    "@storybook/react": "^8.6.11",
    "@storybook/react-vite": "^8.6.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
